{
    "name": "pili-engineering/pili-sdk-php",
    "type": "library",
    "description": "Pili Streaming Cloud server-side library for PHP.",
    "keywords": ["web-service", "rtmp", "hls", "hdl", "hds", "dash", "streaming", "pili", "qiniu", "cloud"],
    "homepage": "https://github.com/pili-engineering/pili-sdk-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Pili Engineering",
            "homepage": "https://github.com/pili-engineering"
        }
    ],
    "support": {
        "issues": "https://github.com/pili-engineering/pili-sdk-php/issues"
    },
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "squizlabs/php_codesniffer": "~2.0"
    },
    "autoload": {
        "psr-0": {
            "Pili": "lib/",
            "Qiniu": "lib/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.5-dev"
        }
    }
}
